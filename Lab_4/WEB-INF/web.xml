<?xml version="1.0" encoding="UTF-8"?> 
<web-app xmlns="http://java.sun.com/xml/ns/javaee" 
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
     xsi:schemaLocation="http://java.sun.com/xml/ns/javaee  
     http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
     version="2.5">

    <display-name>DrekoA4</display-name>

    <welcom-file-list>
    	<welcom-file>index.jsp</welcom-file>
        <welcom-file>index.html</welcom-file>
    </welcom-file-list>

    <servlet>
        <servlet-name>AfternoonServlet</servlet-name>
        <servlet-class>AfternoonServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SecondServlet</servlet-name>
        <servlet-class>SecondServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AfternoonServlet</servlet-name>
        <url-pattern>/AfternoonServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SecondServlet</servlet-name>
        <url-pattern>/SecondServlet</url-pattern>
    </servlet-mapping>
</web-app>