<taglib>
    <tlib-version>1.0</tlib-version>
    <jsp-version>2.0</jsp-version>
    <short-name>custom</short-name>
    <uri>WEB-INF/custom.tld</uri>
    <tag>
        <name>Surname</name>
        <tag-class>stafftag.Surname</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>value</name>
            <required>false</required>
        </attribute>
    </tag>
    <tag>
        <name>Lastname</name>
        <tag-class>stafftag.Lastname</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>value</name>
            <required>false</required>
        </attribute>
    </tag>
    <tag>
        <name>Submit</name>
        <tag-class>stafftag.Submit</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>value</name>
            <required>false</required>
        </attribute>
    </tag>
    <tag>
        <name>Dossier</name>
        <tag-class>stafftag.Dossier</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <name>action</name>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
    </tag>
    <tag>
        <name>Sex</name>
        <tag-class>stafftag.Sex</tag-class>
        <body-content>empty</body-content>
    </tag>
</taglib>